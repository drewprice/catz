var newCat   = $('<%= j render partial: "cat_gif", locals: { cat: @cat } %>').hide()
$('#cat').slideUp(function(){
  $(this).replaceWith(newCat);
  $('#cat').slideDown();
});

var newLike  = $('<%= j render "like" %>')
var newTweet = $('<%= j render "tweet" %>')
$('.like').replaceWith(newLike);
$('.twitter-share-button').replaceWith(newTweet);
